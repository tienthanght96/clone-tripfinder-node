    "@babel/preset-typescript": "^7.10.4",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.9",
    "@types/morgan": "^1.9.2",
    "@types/mysql": "^2.15.15",
